{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "rules": {
    "quote-props": ["error", "as-needed"],
    "object-curly-spacing": ["error", "always"],
    "@typescript-eslint/no-inferable-types": "off"
  }
  // parserOptions: {
  //   ecmaVersion: 2017
